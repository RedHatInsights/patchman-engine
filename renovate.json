{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "run on Monday after 3am and before 10am",
  "schedule": [
    "* 3-10 * * 1"
  ],
  "timezone": "Europe/Prague",
  "semanticCommits": "disabled",
  "extends": [
    "config:recommended"
  ],
  "baseBranchPatterns": [
    "master"
  ],
  "automerge": true,
  "tekton": {
    "automerge": true,
    "automergeStrategy": "rebase",
    "automergeType": "pr",
    "enabled": true,
    "ignoreTests": true,
    "platformAutomerge": true
  },
  "packageRules": []
}
